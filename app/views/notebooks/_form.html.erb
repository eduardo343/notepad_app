<%= form_with(model: notebook, local: true) do |form| %>
    <div class="mb-3">
      <%= form.label :name, "Nombre del notebook", class: "form-label" %>
      <%= form.text_field :name, class: "form-control", placeholder: "Ej: Diario, Ideas, Proyecto X" %>
    </div>
  
    <%= form.submit class: "btn btn-success" %>
  <% end %>
  